{
  "name": "my-workspaces-powered-project",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "my-workspaces-powered-project",
      "workspaces": [
        "package-a",
        "package-b"
      ],
      "dependencies": {
        "package-a": "workspace:*"
      }
    },
    "node_modules/package-a": {
      "resolved": "package-a",
      "link": true
    },
    "node_modules/package-b": {
      "resolved": "package-b",
      "link": true
    },
    "package-a": {
      "version": "1.0.0",
      "license": "ISC"
    },
    "package-b": {
      "version": "1.0.0",
      "license": "ISC",
      "dependencies": {
        "package-a": "^1.0.0"
      }
    }
  },
  "dependencies": {
    "package-a": {
      "version": "file:package-a"
    },
    "package-b": {
      "version": "file:package-b",
      "requires": {
        "package-a": "^1.0.0"
      }
    }
  }
}
